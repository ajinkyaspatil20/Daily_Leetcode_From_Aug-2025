import java.util.*;

public class Solution {
    public List<Boolean> prefixesDivBy5(int[] nums) {
        List<Boolean> res = new ArrayList<>();
        int mod = 0;
        for (int b : nums) {
            mod = ((mod << 1) + b) % 5;
            res.add(mod == 0);
        }
        return res;
    }
}
